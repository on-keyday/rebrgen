    "test_template": "./testkit/cpp3/test_template.cpp",
    "replace_file_name": "stub.cpp",
    "replace_struct_name": "TEST_CLASS",
    "build_output_name": "build_output",
    "build_input_name": "main.cpp",
    "build_command": [
        "python",
        "./testkit/cpp3/setup.py",
        "build",
        "$INPUT",
        "$OUTPUT",
        "$ORIGIN",
